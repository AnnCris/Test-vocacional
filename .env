
# Copia este archivo como .env y ajusta los valores según tu configuración

# Clave secreta para la aplicación (genera una nueva para producción)
SECRET_KEY=clave-secreta-super-segura-para-produccion

# URL de conexión a PostgreSQL
# Formato: postgresql://usuario:contraseña@host:puerto/nombre_base_datos
DATABASE_URL=postgresql://postgres:2458@localhost/test_vocacional

# Configuración de Flask
FLASK_ENV=development
FLASK_APP=app.py
FLASK_DEBUG=1

# Configuración adicional
ML_MODELS_DIR=app/ml_models/saved_models